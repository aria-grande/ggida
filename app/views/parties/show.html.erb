<body>
<div id="kakaoIndex">
  <a href="#kakaoBody">본문 바로가기</a>
  <a href="#kakaoGnb">메뉴 바로가기</a>
</div>
<div id="ggidaWrap" class="cont_sub">
  <div id="ggidaHead" class="ggida_head " role="banner"> <!-- box_visual 영역 사라지면 fixed_head 클래스 추가-->
    <div class="inner_head">
      <h1 class="screen_out">끼니를 다함께</h1>
      <a href="javascript:;" class="btn_ggida btn_navi" style="display:none"><span class="ico_ggida ico_navi">메뉴열기</span></a>
      <div id="ggidaGnb" role="navigation">
        <h2 class="screen_out">끼다 메인메뉴</h2>
        <ul class="gnb_ggida">
          <li><a href="#none" class="link_gnb">HOME</a></li>
          <li><a href="#none" class="link_gnb">모임개설신청</a></li>
          <li><a href="#none" class="link_gnb">끼다소개</a></li>
          <li><a href="#none" class="link_gnb">사용안내</a></li>
          <li><a href="#none" class="link_gnb">문의하기</a></li>
        </ul>
        <button type="button" class="btn_ggida btn_close"><span class="ico_ggida ico_close">메뉴 닫기</span></button>
      </div>
    </div>
  </div>  <!-- //header-->

  <div id="ggidaContent" role="main">
    <div id="cMain">
      <div id="mFeature">
        <div class="side_menu navi_on" style="display:none;left:-1000px"> <!-- navi_on -->
          <ul class="gnb_ggida">
            <li><a href="#none" class="link_gnb">HOME</a></li>
            <li><a href="#none" class="link_gnb">모임개설신청</a></li>
            <li><a href="#none" class="link_gnb">끼다소개</a></li>
            <li><a href="#none" class="link_gnb">사용안내</a></li>
            <li><a href="#none" class="link_gnb">문의하기</a></li>
          </ul>
          <button type="button" class="btn_ggida btn_close"><span class="ico_ggida ico_close">메뉴 닫기</span></button>
        </div>
      </div>
      <div id="mArticle">
        <div class="box_g box_invite">
          <h2 class="screen_out">모임 상세</h2>
          <div class="box_visual">
            <img src="/images/m640/bg_main.png" class="bg_img" alt="">
            <div class="thumb_g thumb_main">
              <img src="http://file.ggida.org/img/<%=@party.id%>_medium_<%=@party.images_file_name%>" class="thumb_img" alt="">
            </div>
          </div>
          <div class="item_invite">
            <strong class="tit_invite"><%=@party.title%></strong>
            <em class="emph_g"><%=@party.left_seats%>명 남았습니다.</em>
            <div class="txt_weite">
              <span class="screen_out">모임소개</span>
              <%= @party.contents %>
            </div>
            <ul class="list_invite">
              <li><em class="em_txt">일자 </em><%= @party.start_date.strftime("%y.%m.%d %H시")%></li>
              <li><em class="em_txt">모집인원 </em>최대 <%= @party.max_participants%>명</li>
              <li><em class="em_txt">참가비 </em><%= @party.price%>원</li>
              <li><em class="em_txt">핸드폰번호 </em><%= @party.contact_number%></li>
              <li><em class="em_txt">이메일 </em><%= @party.contact_email%></li>
              <!--<li><em class="em_txt">일자</em>17.02.28 오후 6시</li>-->
            </ul>
            <div class="info_kitchen">
              <img src="/images/m640/logo_kitchen_03.png" class="thumb_logo" alt="청춘삘-딩">
              <strong class="tit_kitchen"><%=@party.place%></strong>
              <span class="txt_kitchen"><%=@party.address%></span>
            </div>
            <a class="btn_round btn_submit">신청하기</a></li>
          </div>
          <div class="layer_ggida" style="display:none">
            <div class="overlay"></div>
            <div class="inner_layer">
              <div class="box_written">
                <strong class="tit_layer"><em class="emph_g">모임명 </em>채식요리 같이 만들어요!</strong>
                <ul class="list_writte">
                  <li>
                    <div class="item_inp "><label for="" class="lab_weite" >이름</label> <span class="cell_inp"><input id="" type="text" class="inp_weite" placeholder="이름을 입력하세요"/></span></div>
                    <div class="item_inp "><label for="" class="lab_weite" >핸드폰번호</label> <span class="cell_inp"><input id="" type="text" class="inp_weite" placeholder="000-0000-0000"/></span></div>
                    <div class="item_inp "><label for="" class="lab_weite" >이메일</label> <span class="cell_inp"><input id="" type="text" class="inp_weite" placeholder="이메일을 입력하세요"/></span></div>
                  </li>
                </ul>
                <div class="wrap_check check"> <!-- focus 시 .check 추가 -->
                  <input id="checkAgree" name="checkAgree" type="checkbox" class="input_check">
                  <label for="checkAgree" class="lab_check">개인정보제공에 동의합니다.<span class="ico_ggida ico_check"></span></label>
                </div>
              </div>
              <div class="wrap_button2">
                <a class="btn_layer btn_submit">신청</a>
                <a class="btn_layer btn_cancel">취소</a>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="cEtc"></div>
  </div> <!-- //ggidaContent -->

  <div id="ggidaFoot"></div> <!-- //Footer -->
</div>
<script type="text/javascript">
    $(document).ready(function() {
        var Header = (function() {
            var elem = $("#ggidaHead");
            var opener = elem.find(".btn_navi");
            var closeButton = elem.find(".btn_close");
            var navigator = elem.find("#ggidaGnb");
            var logoutButton = elem.find(".btn_logout");
            (function init() {
                opener.click(function() {
                    navigator.show();
                });
                closeButton.click(function() {
                    navigator.hide();
                });
                logoutButton.click(function() {
                    $.ajax( {
                        url: '/managers/sign_out',
                        type: 'DELETE',
                        success: function(result) {
                            window.location.href = "/managers/sign_in";
                        },
                        error: function(error) {
                            console.log(error);
                        }
                    });
                });
            })();
        })();
    });
</script>
</body>
</html>
